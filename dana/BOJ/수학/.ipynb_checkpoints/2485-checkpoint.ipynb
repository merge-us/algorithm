   "source": [
    "import math\n",
    "\n",
    "n = int(input())\n",
    "nums = [int(input()) for _ in range(n)]\n",
    "\n",
    "least_dist = nums[1]-nums[0]\n",
    "for i in range(2,n):\n",
    "    least_dist = math.gcd(nums[i]-nums[i-1],least_dist)\n",
    "\n",
    "count = ((nums[-1]-nums[0])//least_dist+1)-n\n",
    "print(count)"
   ]
