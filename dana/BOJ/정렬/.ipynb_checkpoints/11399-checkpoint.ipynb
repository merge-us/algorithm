    "#한번에 맞춤 크크\n",
    "\n",
    "#사람 수\n",
    "n = int(input())\n",
    "#각 시간 수 \n",
    "data = list(map(int,input().split()))\n",
    "sum = 0\n",
    "\n",
    "data.sort()\n",
    "\n",
    "for i in range(n):\n",
    "    sum+=data[i]*(n-i)\n",
    "print(sum)"
