   "source": [
    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "n, m = map(int, input().rstrip().split())\n",
    "data = set()\n",
    "\n",
    "for _ in range(n):\n",
    "    data.add(input().rstrip())\n",
    "\n",
    "result = 0\n",
    "for _ in range(m):\n",
    "    if input().rstrip() in data:\n",
    "        result += 1\n",
    "\n",
    "print(result)"
   ]
