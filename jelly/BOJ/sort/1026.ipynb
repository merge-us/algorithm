    "n = int(input())\n",
    "\n",
    "a = sorted(map(int, input().split()))\n",
    "b = sorted(map(int, input().split()), reverse=True)\n",
    "\n",
    "result = 0\n",
    "for i in range(n):\n",
    "    result += a[i] * b[i]\n",
    "\n",
    "print(result)"
